{
  "name": "learning-tdd",
  "version": "1.0.0",
  "description": "Learning TDD with kentcdoods's course.",
  "main": "-",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint m2",
    "flow": "flow",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|ts|tsx|md|graphql)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run prettier -- --list-different && npm run flow",
    "precommit": "lint-staged && npm run flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobRuizR/learning-tdd.git"
  },
  "keywords": [
    "jest",
    "testing",
    "kentcdoods",
    "learning"
  ],
  "author": "robruizr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RobRuizR/learning-tdd/issues"
  },
  "homepage": "https://github.com/RobRuizR/learning-tdd#readme",
  "dependencies": {
    "eslint": "^5.15.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-config-prettier": "^4.1.0",
    "flow-bin": "^0.95.1",
    "husky": "^1.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^1.16.4"
  }
}
